<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".setwallpaper">

    <ImageView
        android:id="@+id/finalimage"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:scaleType="centerCrop"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="0dp">

    </ImageView>

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/btndownload"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_margin="40dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="@color/setbtn"
        app:icon="@drawable/ic_download"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />


    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/btnbookmark"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_margin="40dp"
        android:backgroundTint="@color/setbtn"
        app:icon="@drawable/ic_wishlist"
        app:layout_constraintBottom_toTopOf="@+id/btndownload"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

    <com.google.android.material.floatingactionbutton.ExtendedFloatingActionButton
        android:id="@+id/btnset"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_margin="40dp"
        android:backgroundTint="@color/setbtn"
        app:icon="@drawable/ic_setw"
        app:layout_constraintBottom_toTopOf="@+id/btnbookmark"
        app:layout_constraintEnd_toEndOf="parent"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>